{
    "name": "claserre9/doctrine-generic-filter",
    "description": "Provide a simple mechanism to filter doctrine queries",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "doctrine",
        "orm",
        "generic",
        "filter"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Clifford Laserre",
            "email": "claserre9@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.11.0",
        "doctrine/dbal": "^3.2",
        "symfony/cache": "^6.3"
    },
    "require-dev": {

        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test" : " php vendor/bin/phpunit -c phpunit.xml --testdox tests",
        "lint" : "vendor/bin/phpcs src --severity=0 --extensions=php",
        "lint-fix" : "vendor/bin/phpcbf src"
    }
}
